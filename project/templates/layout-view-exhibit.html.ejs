<!DOCTYPE html>
<html lang="en">

    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Exhibits</title>
        <style>
            ul {list-style-type: none;}
        </style>
        <link href="site.css" rel="stylesheet" type="text/css" />
        <link rel="icon" href="../assets/buttons/icons8-button7-64.png" type="image/png" />
    </head>

    <body>
        
        <section id="page">       
            <nav><%- navi %></nav>
            <header>Gallery</header>
            <main> 
            List all exhibit artifacts

            <ul>              
            <% exhibitArtifacts.forEach((artifact) => { %>
                <li>
                    <a href="/../../artifact/+<%= artifact.ArtifactID %>" >
                        <img src="../static/assets/buttons/logo-draft.png">
                    </a>
                </li>
            <% }); %>
            </ul>
           
            </main>
        </section>
        

        <link rel="stylesheet" href="site.css" type="text/css"/>
        <script src="site.js"></script>

    </body>
</html>